    "jest": "^29.7.0",
    "nyc": "^15.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsd": "^0.30.7",
    "tsyringe": "^4.8.0",
    "typescript": "^5.4.2",
    "typescript-ioc": "^3.2.2"
  },
  "prettier": {
    "singleQuote": true,
